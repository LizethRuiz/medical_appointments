                            "email": "juanperez@gmail.com"
                          }
                        }
                      }
                    ]
                  }
                }
              }
            }
          },
          "401": {
            "description": "This endpoint is secured - **Token required**."
          },
          "default": {
            "description": "Unexpected response - internal error"
          }
        },
        "security": [{ "token": ["read"] }]
      },
      "post": {
        "tags": ["Appointments"],
        "operationId": "createAppointment",
        "summary": "Add Appointment",
        "description": "As patient user you can schedule a appointment",
        "requestBody": {
          "description": "Data to create a appointment",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "startDate": {
                    "type": "date",
                    "example": "2023-05-21T21:20:00.000Z"
                  },
                  "doctorId": {
                    "type": "integer",
                    "example": 3
                  }
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Information has been added",
              "content": {
                "application/json": {
                  "schema": {
                    "properties": {
                      "startDate": {
                        "type": "date",
                        "example": "2023-05-20T20:20:00.000Z"
                      },
                      "endDate": {
                        "type": "date",
                        "example": "2023-05-20T21:00:00.000Z"
                      },
                      "doctorId": {
                        "type": "integer",
                        "example": 3
                      },
                      "patientId": {
                        "type": "integer",
                        "example": 3
                      },
                      "notes": {
                        "type": "string",
                        "example": null
                      }
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid request",
              "schema": {
                "type": "object",
                "example": [
                  {
                    "errors": "Date and hour are required"
                  },
                  {
                    "errors": "The doctor Id is required"
                  },
                  {
                    "errors": "Doctor registry not found"
                  },
                  {
                    "errors": "This schedule has already been taken"
                  }
                ]
              }
            },
            "401": {
              "description": "This endpoint is secured - **Token required**."
            },
            "default": {
              "description": "Unexpected response - internal error"
            }
          },
          "security": [{ "token": ["write"] }]
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": { "bearerAuth": [] },
  "parameters": {
    "page": {
      "in": "query",
      "description": "Number of page",
      "name": "page",
      "required": false,
      "type": "integer"
    },
    "pageSize": {
      "in": "query",
      "description": "Length of the pagination",
      "name": "pageSize",
      "required": false,
      "type": "integer"
    },
    "month": {
      "in": "query",
      "description": "Month and Year filter. Both fields are considered to filter in that range, Ejemplo: 3",
      "name": "month",
      "required": false,
      "type": "number"
    },
    "year": {
      "in": "query",
      "description": "Month and Year filter. Both fields are considered to filter in that range, Ejemplo: 2023",
      "name": "year",
      "required": false,
      "type": "number"
    },
    "date": {
      "in": "query",
      "description": "To filter a specific day use query date, Ejemplo: 2023-03-15T20:00:00.000Z",
      "name": "date",
      "required": false,
      "type": "string"
    },
    "id": {
      "in": "path",
      "description": "ID of data object, e.g. \"1\"",
      "name": "id",
      "required": true,
      "type": "string"
    },
    "DataObject": {
      "in": "body",
      "description": "Data to save with given ID",
      "name": "data",
      "required": true,
      "schema": {
        "type": "object",
        "properties": { "name": { "type": "string", "example": "John Doe" } }
      }
    }
  }
}
